<footer class="ws-footer" aria-label="WoodenStreet Footer Clone">
  <!-- Trust Strip -->
  <div class="ws-footer__trust-strip">
    <div class="page-width">
      <div class="grid">
        <div class="ws-trust-item">
          <div class="ws-trust-item__icon">
            <svg class="w-10 h-10" fill="none" stroke="currentColor" viewBox="0 0 24 24"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5" d="M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z"></path></svg>
          </div>
          <h4>100% Quality</h4>
          <p>Inspected at Every Step</p>
        </div>
        <div class="ws-trust-item">
          <div class="ws-trust-item__icon">
            <svg class="w-10 h-10" fill="none" stroke="currentColor" viewBox="0 0 24 24"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5" d="M12 8v4l3 3m6-3a9 9 0 11-18 0 9 9 0 0118 0z"></path></svg>
          </div>
          <h4>Fast Delivery</h4>
          <p>Across 300+ Cities</p>
        </div>
        <div class="ws-trust-item">
          <div class="ws-trust-item__icon">
            <svg class="w-10 h-10" fill="none" stroke="currentColor" viewBox="0 0 24 24"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5" d="M3 10h18M7 15h1m4 0h1m-7 4h12a3 3 0 003-3V8a3 3 0 00-3-3H6a3 3 0 00-3 3v8a3 3 0 003 3z"></path></svg>
          </div>
          <h4>Secure Payment</h4>
          <p>SSL Encrypted Gateway</p>
        </div>
        <div class="ws-trust-item">
          <div class="ws-trust-item__icon">
             <svg class="w-10 h-10" fill="none" stroke="currentColor" viewBox="0 0 24 24"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5" d="M18.364 5.636l-3.536 3.536m0 5.656l3.536 3.536M9.172 9.172L5.636 5.636m3.536 9.192l-3.536 3.536M21 12a9 9 0 11-18 0 9 9 0 0118 0zm-5 0a4 4 0 11-8 0 4 4 0 018 0z"></path></svg>
          </div>
          <h4>24/7 Support</h4>
          <p>We are here to help</p>
        </div>
      </div>
    </div>
  </div>

  <!-- Main Footer -->
  <div class="ws-footer__main">
    <div class="page-width">
      <div class="grid">
        <!-- Brand Column -->
        <div class="ws-footer-brand">
          <h3>WoodenStreet</h3>
          <p>
            Experience the finest wood furniture collection at our online furniture store. We provide durable and stylish home furniture for every corner of your room.
          </p>
          <div class="ws-footer-social">
             <!-- Social Icons -->
             <a href="#" aria-label="Facebook">f</a>
             <a href="#" aria-label="Twitter">t</a>
             <a href="#" aria-label="Instagram">i</a>
          </div>
        </div>

        {% for i in (1..3) %}
          <div class="ws-footer-links">
            <h4>User Links</h4>
            <ul>
              <li><a href="#">About Us</a></li>
              <li><a href="#">Our Stories</a></li>
              <li><a href="#">Experience Stores</a></li>
              <li><a href="#">Interior Design</a></li>
            </ul>
          </div>
        {% endfor %}
      </div>

      <div class="ws-footer-bottom">
        <p>Â© {{ 'now' | date: "%Y" }} WoodenStreet. All rights reserved.</p>
        <div class="ws-footer-payments">
           <!-- Payment Icons -->
           <img src="https://checkout.stripe.com/img/v3/home/logos/visa.svg" class="h-4" loading="lazy" alt="Visa">
           <img src="https://checkout.stripe.com/img/v3/home/logos/mastercard.svg" class="h-4" loading="lazy" alt="Mastercard">
        </div>
      </div>
    </div>
  </div>
</footer>

<style>
  .ws-footer a { transition: 0.3s; }
</style>

{% schema %}
{
  "name": "WS Premium Footer",
  "settings": [],
  "presets": [
    {
      "name": "WS Premium Footer"
    }
  ]
}
{% endschema %}
