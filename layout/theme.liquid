    {% comment %} Ecommerce-App Theme Initialization (Dark Mode Support) {% endcomment %}
    <script>
      (function() {
        const savedTheme = localStorage.getItem('bluewud_theme');
        let mode = 'dark'; // Default to dark as per wireframe.onslate.com
        
        if (savedTheme) {
          try {
            const theme = JSON.parse(savedTheme);
            mode = theme.mode || 'dark';
          } catch (e) {}
        }

        document.documentElement.setAttribute('data-theme', mode);
        window.initialThemeMode = mode;
      })();
